{
    "compilerOptions": {
        "allowJs": true,
        "alwaysStrict": true,
        "baseUrl": "./",
        "declaration": true,
        "forceConsistentCasingInFileNames": true,
        "incremental": true,
        "lib": [
            "ES2022"
        ],
        "module": "ES2022",
        "moduleResolution": "node",
        "noEmit": true,
        "noImplicitAny": true,
        "sourceMap": true,
        "strict": true,
        "target": "ES2022"
    }
}
